{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "off"
            }
        ],
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off"
    }
}
